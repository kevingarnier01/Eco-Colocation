
@{
	ViewBag.Title = "ModalCreateAccount";
}
<div class="modal">
	<div class="underModal col-sm-12 col-xs-12">
		<div id="modalCreateAccount" class="reelModal">
			<div>
				<div id="divInscriptionTitle-mca" style="display: flex;margin-bottom: 20px;">
					<h3 style="margin-left: auto;margin-right: auto;font-family: Poppins-Bold;font-weight: bold;font-size: 24px;color: #333;">
						Inscription du membre
					</h3>
				</div>
				<div>
					<div class="divParentTypeInterest-mca" style="display: flex;">
						<a href="@Url.Action("ModalCARecherche","Account", null)" rel="modal:open" onclick="openSecondModal()"><div>Je recherche une éco-colocation</div></a>
					</div>
					<div class="divParentTypeInterest-mca" style="display: flex;">
						<a href="@Url.Action("ModalCAPropose","Account", null)" rel="modal:open" onclick="openSecondModal()"><div>Je propose une éco-colocation</div></a>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>